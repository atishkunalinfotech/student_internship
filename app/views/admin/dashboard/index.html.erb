<div id="faq">
    <div class="container" style="margin-top:150px">
        <div class="section_header">
            <h3>Dashboard</h3>
        </div>

        <div class="row">
            <div class="span6 block">
              <div class="box">
                    <p class="title">Welcome to Dashboard</p>
                    
                   
              </div>
            </div>

            <div class="span6 block">

            <%if current_user.role == "admin"%>
              <div class="box">
			          <ul>
			              <h3>Menu 1</h3>
			              <li><%= link_to "Job Group",admin_job_groups_path%></li>
                    <li><%= link_to "Faculty Account",admin_users_path(:role => "faculty")%></li>
                    <li><%= link_to "Staff Account",admin_users_path(:role => "staff")%></li>
                    <li><%= link_to "Add City",admin_cities_path%></li>
					      
					      <%#= link_to "Internship Status", admin_internship_statuses_path %>
                <h3>Menu 2</h3>
					      <li><%= link_to "Companies",admin_companies_path%></li>
					      
					      <%#= link_to "Skill Group",admin_skill_groups_path%>
					      <%#= link_to "Skills",admin_skills_path%>
					      <li><%= link_to "Students",admin_students_path%></li>
					      <!-- <li><%= link_to "Job Posting",admin_job_posts_path%></li> -->
                <li><%= link_to "Jobs",admin_jobs_path%></li>
					      <%#= link_to "Intrested Students For Job",admin_student_job_interests_path%>
					      <%#= link_to "Student Job Hire",admin_student_hire_path%>
					      <%#= link_to "Reports",admin_reports_path%>

                </ul>
                
              </div>
            <%elsif current_user.role == "faculty" %>
                <%= link_to "Faculty change password",admin_user_change_password_path(:user_id => current_user.id)%>
            <%else%>
                 <%= link_to "Staff change password",admin_user_change_password_path(:user_id => current_user.id)%>
            <%end%>
                
            </div>

        </div>
    </div>
</div>





